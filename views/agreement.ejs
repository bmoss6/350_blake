									<!-- Features -->
										<section class="box features">
											<h2 class="major"><span>Engagement Report</span></h2>
											<ul>
												<h2>Agreement ID: <%=agreement[0].agreement_id %></h2>
												<h2>Report Date: <%=agreement[0].effective_date %></h2>
												<h2>Signer: <%=agreement[0].signer %></h2>
												<h2>Scope: <%=agreement[0].scope %></h2>
											</ul>
												<div class="row">
													<ul>
													<% for(var i=0; i<engagementinfo.length; i++) {%>
													<div class="3u 12u(mobile)">
													</div>

													<% } %>

															</ul>
												</div>

												<table class="responstable">

													<tr>
														<th><b> <u> Vulnerability</u></b></th>

														<th><b> <u>Action</u></b></th>
														<th><b> <u>Status</u></b></th>

													</tr>
													<div class="row">

														<% for(var i=0; i<engagementinfo.length; i++) {%>
															<tr>
																<td align="center"><%=engagementinfo[i].vulnerability %></td>
																<td align="center"><%=engagementinfo[i].action %></td>

																<%if (engagementinfo[i].status=='Open') { %>
																	<td align ="center"><button class= "open"><a href ="/update/<%=engagementinfo[i].vulnerability %>/<%=engagementinfo[i].agreement_id %>/Remediated"><%=engagementinfo[i].status %></a></button></td>
																		<% } %>
																		<%if (engagementinfo[i].status=='Remediated') { %>
																			<td align ="center"> <button><a href ="/update/<%=engagementinfo[i].vulnerability %>/<%=engagementinfo[i].agreement_id %>/Open"><%=engagementinfo[i].status %></a></button></td>
																				<% } %>

															</tr>

														<% } %>








												</table>






